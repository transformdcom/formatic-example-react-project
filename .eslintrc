{
  "plugins": ["jest"],
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "operator-linebreak": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0
  },
  "globals": {
    "document": true,
    "fetch": true,
    "localStorage": true,
    "navigator": true,
    "sessionStorage": true,
    "window": true
  }
}
